<div id="home">
    <div class="main-text">
        <div class="main-name">
            <h2><%= t("home.name") %></h2>
        </div>
        <div class="main-personal">
            <h3><%= t("home.title").upcase %></h3>
        </div>
    </div>
</div>
<main class="section-container">
    <div class="item section left about">
        <div id="about">
            <h2 class="title about"><%= t("about.title").upcase %></h2>
            <p class="text about"><%= t("about.content") %></p>
        </div>
    </div>
    <div class="item">
        <img src="https://images.unsplash.com/photo-1616279969856-759f316a5ac1?ixid=MnwxMjA3fDB8MHxzZWFyY2h8NHx8cGVyc29uYWwlMjB0cmFpbmluZ3xlbnwwfHwwfHw%3D&ixlib=rb-1.2.1&auto=format&fit=crop&w=500&q=60" alt="szemelyi edzes" class="section-picture left">
    </div>
    <div class="item section right coaching">
        <div id="coaching">
            <h2 class="title coaching"><%= t("coaching.title") %></h2>
            <p class="text coaching"><%= t("coaching.content") %></p>
        </div>
    </div>
    <div class="item">
        <img src="https://images.unsplash.com/photo-1586083702768-190ae093d34d?ixid=MnwxMjA3fDB8MHxzZWFyY2h8NHx8bWFufGVufDB8fDB8fA%3D%3D&ixlib=rb-1.2.1&auto=format&fit=crop&w=500&q=60" alt="Szoke Csaba" class="section-picture right">
    </div>
    <div class="item section left training">
        <div id="training">
            <p class="text training"><%= t("training.content") %></p>
            <h2 class="title training"><%= t("training.title") %></h2>
        </div>
    </div>
    <div class="item">
        <img src="https://images.unsplash.com/photo-1527856263669-12c3a0af2aa6?ixid=MnwxMjA3fDB8MHxzZWFyY2h8Nnx8bGlmZSUyMGNvYWNofGVufDB8fDB8fA%3D%3D&ixlib=rb-1.2.1&auto=format&fit=crop&w=500&q=60" alt="coaching" class="section-picture left">
    </div>
</main>